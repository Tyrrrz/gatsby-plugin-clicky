{
  "name": "gatsby-plugin-clicky",
  "version": "1.0.0",
  "description": "Gatsby integration for Clicky analytics (https://clicky.com)",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tyrrrz/gatsby-plugin-clicky.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "clicky",
    "analytics",
    "stats"
  ],
  "author": "Alexey 'Tyrrrz' Golub",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Tyrrrz/gatsby-plugin-clicky/issues"
  },
  "homepage": "https://github.com/Tyrrrz/gatsby-plugin-clicky#readme",
  "dependencies": {
    "@babel/runtime": "^7.7.2"
  },
  "devDependencies": {
    "prettier": "^1.19.1",
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@testing-library/react": "^9.3.2",
    "babel-preset-gatsby-package": "^0.2.11",
    "cross-env": "^5.2.1"
  },
  "peerDependencies": {
    "gatsby": "^2.18.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  }
}
